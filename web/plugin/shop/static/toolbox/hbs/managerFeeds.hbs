<div class="row">
    <div class="col-md-6">
        <div class="panel panel-green">
            <div class="panel-heading">
                <i class="fa fa-plus fa-fw generate text-info"></i>
                <span class="title">{{lang.managerFeeds.titleImport}}</span>
            </div>
            <div class="panel-body">
                <ul class="list-group feeds feeds-generated">
                {{#each data}}
                    {{#if_eq type 'generated'}}
                    <a class="list-group-item feed-item" href="{{link}}" target="_blank" title="{{name}}" data-id="{{ID}}">
                        <span class="time badge pull-left">{{timeFormatted}}</span>
                        <span class="name">{{name}}</span>
                        <i class="fa fa-download pull-right text-info"></i>
                    </a>
                    {{/if_eq}}
                {{/each}}
                </ul>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="panel panel-yellow">
            <div class="panel-heading">
                <span class="fileinput-button btn-feed-upload">
                    <i class="fa fa-plus fa-fw text-info"></i>
                    <input type="file" id="fileFeed">
                </span>
                <span class="title">{{lang.managerFeeds.titleExport}}</span>
            </div>
            <div class="panel-body">
                <ul class="list-group feeds feeds-uploaded">
                {{#each data}}
                    {{#if_eq type 'uploaded'}}
                    <a class="list-group-item feed-item" href="javascript://" target="_blank" title="{{name}}" data-id="{{ID}}">
                        <span class="time badge pull-left">{{timeFormatted}}</span>
                        <span class="name">{{name}}</span>
                        <i class="fa fa-play pull-right text-info start-import"></i>
                    </a>
                    {{/if_eq}}
                {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>