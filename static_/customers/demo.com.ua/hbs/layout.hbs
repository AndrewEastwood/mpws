                <div class="element clearfix col2-3 info white"><p class="mpws-js-info-warranty"></p></div>
                <div class="element clearfix col1-3 info white"><p class="mpws-js-info-contacts"></p></div>
                <div class="element clearfix col3-3 cart white"><p class="mpws-js-widget-cart"></p></div>

            </div>
        </div>
        <!-- end container -->
    </div>

    <!-- end content -->
    <footer>
