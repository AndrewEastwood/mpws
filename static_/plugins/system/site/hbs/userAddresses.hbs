<h3>{{lang.profile_page_addresses_title}}</h3>
<div class="account-addresses-buttons">
    <button class="btn btn-success btn-xs add-address">
        <i class="fa fa-plus fa-fw"></i>
        {{lang.profile_page_addresses_button_addAddress}}
    </button>
</div>
{{#if data.success }}
    <div class="alert alert-success">{{lang.profile_page_addresses_message_saveSuccess}}</div>
{{else}}
    {{#mpwsIsNotEmpty data.error }}
        <div class="alert alert-danger">{{lang.profile_page_addresses_message_saveError}}</div>
    {{/mpwsIsNotEmpty}}
{{/if}}
<div class="account-addresses"></div>