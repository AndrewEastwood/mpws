<div class="row clearfix hProductItems" itemscope itemtype="http://data-vocabulary.org/Product">
    <div class="col-xs-0 col-sm-0 col-md-2"></div>
    <div class="col-xs-12 col-sm-12 col-md-8">

        <div class="thumbnail">
            <a href=""><img src="" alt="" itemprop="image"></a>
        </div>
        <div class="thumbSetting">
            <div class="thumbTitle">
                <a href="#" class="invarseColor" itemprop="name">{{Name}}</a>
            </div>
            <meta itemprop="currency" content="USD" />
            <div class="thumbPrice"><span itemprop="price" class='datatype-money datatype-money-usd'>{{Price}}</span></div>

            <div class="thumbButtons">
                <button class="btn btn-primary btn-small" data-title="+To Cart" data-placement="top" data-toggle="tooltip" data-original-title="" title="">
                    <i class="icon-shopping-cart"></i>
                </button>
                <button class="btn btn-small" data-title="+To WishList" data-placement="top" data-toggle="tooltip" data-original-title="" title="">
                    <i class="icon-heart"></i>
                </button>
                <button class="btn btn-small" data-title="+To Compare" data-placement="top" data-toggle="tooltip" data-original-title="" title="">
                    <i class="icon-refresh"></i>
                </button>
            </div>

            <ul class="rating">
                <li><i class="star-on"></i></li>
                <li><i class="star-on"></i></li>
                <li><i class="star-on"></i></li>
                <li><i class="star-on"></i></li>
                <li><i class="star-off"></i></li>
            </ul>
        </div>

    </div>
    <div class="col-xs-0 col-sm-0 col-md-2"></div>
</div>